{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "lib/**", "build/**"]
        },
        "lint": {
            "dependsOn": ["^lint"],
            "outputs": []
        },
        "test": {
            "dependsOn": ["^test"],
            "outputs": []
        }
    }
}
